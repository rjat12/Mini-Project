  <script>

    function remove_item(item_id) {
      if (confirm("Are you sure you want to remove it?")) {
        window.location.href = `/remove_item?id=${item_id}`;
      }
    }
    function check_quan(item_id,item_quantity){
      cart.forEach(element => {
        console.log(element.id);
        if(element.id===item_id){
          if(element.quantity>item_quantity){
            window.alert("Insufficient Quantity")
          }
          else{
            window.location.href = `/add_cart`;
          }
        }
      });
    }
    function getCookie() {
      let decodedCookie = document.cookie;
      let ca = decodedCookie.split('email=');
      let c = ca[1];
      let c1= c.replace("%40","@");
      document.getElementById("email").innerHTML=c1;
    }

  </script>
